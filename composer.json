{
    "name": "joecianflone/supermodules",
    "description": "A highly configurable module generator system for laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JoeCianflone\\SuperModules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoeCianflone\\SuperModules\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joe Cianflone",
            "email": "joe@cianflone.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.45 || ^11.0",
        "illuminate/contracts": "^10.45 || ^11.0",
        "illuminate/filesystem": "^10.45 || ^11.0",
        "composer/composer": "^2.7",
        "illuminate/support": "^10.45 || ^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.10",
        "orchestra/testbench": "^8.21",
        "pestphp/pest": "^2.34",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoeCianflone\\SuperModules\\SuperModulesServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test:static": "vendor/bin/phpstan analyse",
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
